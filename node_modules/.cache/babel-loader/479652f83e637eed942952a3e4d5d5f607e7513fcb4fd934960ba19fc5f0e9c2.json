{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport { useRoute } from 'vue-router';\nimport { useTodosStore } from '@/stores/todosStore';\nimport { computed, onMounted } from 'vue';\nexport default {\n  setup() {\n    const route = useRoute();\n    const userId = parseInt(route.params.id, 10); // Dinamik rota parametresi\n    const todosStore = useTodosStore();\n\n    // Kullanıcının Todos verilerini filtrele\n    const todos = computed(() => todosStore.todos.filter(todo => todo.userId === userId));\n    onMounted(() => {\n      if (!todosStore.todos.length) {\n        todosStore.fetchTodos(); // API çağrısı\n      }\n    });\n    return {\n      todos,\n      userId\n    };\n  }\n};","map":{"version":3,"names":["useRoute","useTodosStore","computed","onMounted","setup","route","userId","parseInt","params","id","todosStore","todos","filter","todo","length","fetchTodos"],"sources":["C:\\Users\\ismail\\Downloads\\manage-deneme\\manage-deneme\\vue-tailwind-dashboard\\src\\pages\\TodosPage.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h1 class=\"text-2xl font-semibold mb-6\">Todos for User {{ userId }}</h1>\r\n      <ul class=\"space-y-4\">\r\n        <li\r\n          v-for=\"todo in todos\"\r\n          :key=\"todo.id\"\r\n          class=\"flex items-center space-x-4\"\r\n        >\r\n          <input\r\n            type=\"checkbox\"\r\n            :checked=\"todo.completed\"\r\n            class=\"form-checkbox text-purple-600\"\r\n          />\r\n          <p\r\n            :class=\"{ 'line-through text-gray-400': todo.completed }\"\r\n            class=\"text-gray-800\"\r\n          >\r\n            {{ todo.title }}\r\n          </p>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { useRoute } from 'vue-router';\r\n  import { useTodosStore } from '@/stores/todosStore';\r\n  import { computed, onMounted } from 'vue';\r\